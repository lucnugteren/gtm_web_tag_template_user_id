___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const setCookie = require("setCookie");
const getCookieValues = require("getCookieValues");
const generateRandom = require("generateRandom");
const addConsentListener = require("addConsentListener");
const isConsentGranted = require("isConsentGranted");

const COOKIE_NAME = "__ln_uid";
const COOKIE_OPTIONS = {
  domain: "auto",
  path: "/",
  "max-age": 60 * 60 * 24 * 365,
  samesite: "Lax",
  secure: false
};

let consentHandled = false;

const generateUUID = () => {
  const generateRandomHex = (length) => (function() {
    let hexString = "";
    for (let i = 0; i < length; i++) {
      hexString += generateRandom(0, 15).toString(16);
    }
    return hexString;
  })();
  const getRandom89AB = () => (generateRandom(0, 3) + 8).toString(16);
  return (
    generateRandomHex(8) +
    "-" + generateRandomHex(4) +
    "-" + "4" + generateRandomHex(3) +
    "-" + getRandom89AB() + generateRandomHex(3) +
    "-" + generateRandomHex(12)
  );
};

const handleAnalyticsConsent = () => {
  if (consentHandled) return;

  if (isConsentGranted("analytics_storage")) {
    let lnUserId = getCookieValues(COOKIE_NAME)[0];
    if (!lnUserId) {
      lnUserId = generateUUID();
      setCookie(COOKIE_NAME, lnUserId, COOKIE_OPTIONS);
    }
    consentHandled = true;
  }

  data.gtmOnSuccess();
};

handleAnalyticsConsent();

if (!consentHandled) {
  addConsentListener("analytics_storage", handleAnalyticsConsent);
}


